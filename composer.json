{
    "name": "104corp/monolog-extensions",
    "description": "The extensions for monolog",
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "104corp/support": "^1.2.1",
        "monolog/monolog": "^2",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^8 | ^9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Corp104\\Monolog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
