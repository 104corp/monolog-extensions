{
  "name": "104corp/monolog-handler",
  "description": "The handlers for monolog",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=7",
    "monolog/monolog": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Corp104\\Monolog\\": "src/Monolog"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  }
}
