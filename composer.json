{
    "name": "104corp/monolog-extensions",
    "description": "The extensions for monolog",
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "104corp/support": "^1.2.1",
        "monolog/monolog": "^1.24"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": ">4.8 <6",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Corp104\\Monolog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
